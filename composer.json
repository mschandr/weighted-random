{
  "name": "mschandr/weighted-random",
  "description": "Library for generating a weighted random sample",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "weighted",
    "random",
    "sampling"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
    "beberlei/assert": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "mschandr\\WeightedRandom\\": "lib/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "^3.3",
    "vimeo/psalm": "^3.4"
  }
}

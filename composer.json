{
    "name": "mschandr/weighted-random",
    "description": "Library for generating a weighted random sample",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Houweling"
        },
        {
            "name": "Mark Dhas",
            "email": "mark.dhas@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "weighted",
        "random",
        "sampling"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "mschandr\\WeightedRandom\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.87",
        "ergebnis/composer-normalize": "^2.48",
        "phpunit/phpcov": "^11.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "coverage": "./vendor/bin/pcov"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
